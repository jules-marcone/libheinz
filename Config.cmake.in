# Compute paths
get_filename_component(LibHeinz_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(LibHeinz_INCLUDE_DIRS @CMAKE_INSTALL_PREFIX@/include)

